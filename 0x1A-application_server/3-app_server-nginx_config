server {
    listen 80;
    listen localhost:80;
    server_name 34.232.53.87;  # Replace with your actual domain or IP

    location /airbnb-onepage/ {
        proxy_pass http://127.0.0.1:5000;  # Replace with your Gunicorn host:port
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
    location ~ ^/airbnb-dynamic/number_odd_or_even/([0-9]+)$ {        proxy_pass http://127.0.0.1:5001/number_odd_or_even/$1;  # Proxy to process on port 6000
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }
}
